{
  "name": "jeboehm/mailserver-admin",
  "description": "This is an administration interface for docker-mailserver.",
  "type": "project",
  "license": "MIT",
  "require": {
    "php": ">=8.4",
    "ext-iconv": "*",
    "ext-openssl": "*",
    "composer/ca-bundle": "1.5.10",
    "doctrine/dbal": "4.4.1",
    "doctrine/doctrine-bundle": "3.2.2",
    "doctrine/doctrine-migrations-bundle": "3.7.0",
    "doctrine/orm": "3.6.2",
    "easycorp/easyadmin-bundle": "v4.28.0",
    "hwi/oauth-bundle": "v2.4.0",
    "predis/predis": "v3.3.0",
    "symfony/asset-mapper": "*",
    "symfony/console": "v7.4.4",
    "symfony/dotenv": "v7.4.0",
    "symfony/flex": "v2.10.0",
    "symfony/form": "7.4.4",
    "symfony/framework-bundle": "v7.4.5",
    "symfony/http-client": "v7.4.5",
    "symfony/intl": "7.4.4",
    "symfony/monolog-bundle": "v3.11.1",
    "symfony/proxy-manager-bridge": "v6.4.28",
    "symfony/runtime": "v7.4.1",
    "symfony/security-bundle": "v7.4.4",
    "symfony/serializer": "v7.4.5",
    "symfony/stimulus-bundle": "*",
    "symfony/twig-bundle": "v7.4.4",
    "symfony/ux-chartjs": "^2.32",
    "symfony/validator": "v7.4.5",
    "symfony/yaml": "v7.4.1",
    "twig/extra-bundle": "v3.23.0",
    "twig/string-extra": "v3.23.0"
  },
  "require-dev": {
    "dama/doctrine-test-bundle": "^v8.2.0",
    "friendsofphp/php-cs-fixer": "v3.93.1",
    "phpunit/phpunit": "^12.0",
    "rector/rector": "2.3.5",
    "roave/security-advisories": "dev-latest",
    "symfony/browser-kit": "v7.4.4",
    "symfony/css-selector": "v7.4.0",
    "symfony/phpunit-bridge": "v7.4.3",
    "symfony/stopwatch": "v7.4.0",
    "symfony/web-profiler-bundle": "v7.4.4"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true,
    "allow-plugins": {
      "symfony/flex": true,
      "symfony/runtime": true
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "replace": {
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php82": "*",
    "symfony/polyfill-php81": "*",
    "symfony/polyfill-php80": "*",
    "symfony/polyfill-php74": "*",
    "symfony/polyfill-php73": "*",
    "symfony/polyfill-php72": "*",
    "symfony/polyfill-php71": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "importmap:install": "symfony-cmd",
      "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ],
    "csfix": "php-cs-fixer fix",
    "rector": "vendor/bin/rector process",
    "test": "phpunit --display-all-issues",
    "coverage": "@php -d xdebug.mode=coverage vendor/bin/phpunit --coverage-html=_coverage",
    "phpstan": "@php -d memory_limit=512M vendor/bin/phpstan analyse --level 3",
    "create-tls": "if ! [ -d tls ] && ! [ -f tls/tls.key ] && ! [ -f tls/tls.crt ]; then mkdir tls; fi && openssl req -x509 -newkey rsa:1024 -keyout tls/tls.key -out tls/tls.crt -sha256 -days 365 -nodes -subj \"/C=XX/ST=StateName/L=CityName/O=CompanyName/OU=CompanySectionName/CN=CommonNameOrHostname\""
  },
  "conflict": {
      "symfony/symfony": "*"
  },
  "extra": {
      "symfony": {
          "allow-contrib": false
      }
  }
}
