{
  "name": "jeboehm/mailserver-admin",
  "description": "This is an administration interface for docker-mailserver.",
  "type": "project",
  "license": "MIT",
  "require": {
    "php": ">=8.3",
    "ext-iconv": "*",
    "ext-openssl": "*",
    "doctrine/dbal": "4.0.5",
    "doctrine/doctrine-bundle": "2.12.0",
    "doctrine/doctrine-migrations-bundle": "3.3.1",
    "doctrine/orm": "3.2.1",
    "easycorp/easyadmin-bundle": "v4.11.1",
    "symfony/console": "v6.4.10",
    "symfony/flex": "v2.4.6",
    "symfony/framework-bundle": "v6.4.10",
    "symfony/proxy-manager-bridge": "v6.4.8",
    "symfony/runtime": "v6.4.8",
    "symfony/security-bundle": "v6.4.10",
    "symfony/twig-bundle": "v6.4.8",
    "symfony/validator": "v6.4.10",
    "symfony/yaml": "v6.4.8"
  },
  "require-dev": {
    "dama/doctrine-test-bundle": "^v8.2.0",
    "friendsofphp/php-cs-fixer": "v3.62.0",
    "phpunit/phpunit": "^11.0",
    "rector/rector": "1.2.2",
    "symfony/browser-kit": "v6.4.8",
    "symfony/css-selector": "v6.4.8",
    "symfony/dotenv": "v6.4.10",
    "symfony/phpunit-bridge": "v6.4.10",
    "symfony/stopwatch": "v6.4.8",
    "symfony/web-profiler-bundle": "v6.4.10"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true,
    "allow-plugins": {
      "symfony/flex": true,
      "symfony/runtime": true
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php74": "*",
    "symfony/polyfill-php73": "*",
    "symfony/polyfill-php72": "*",
    "symfony/polyfill-php71": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ],
    "csfix": "php-cs-fixer fix --allow-risky=yes",
    "test": "phpunit"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false
    }
  }
}
