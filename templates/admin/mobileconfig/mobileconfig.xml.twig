<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>PayloadContent</key>
    <array>
        <dict>
            <key>EmailAccountDescription</key>
            <string>{{ companyName|e }}</string>
            <key>EmailAccountName</key>
            <string>{{ email|e }}</string>
            <key>EmailAccountType</key>
            <string>EmailTypeIMAP</string>
            <key>EmailAddress</key>
            <string>{{ email|e }}</string>
            <key>IncomingMailServerAuthentication</key>
            <string>EmailAuthPassword</string>
            <key>IncomingMailServerHostName</key>
            <string>{{ mailServerHost|e }}</string>
            <key>IncomingMailServerPortNumber</key>
            <integer>{{ imapPort }}</integer>
            <key>IncomingMailServerUseSSL</key>
            <{{ portsAreSsl ? 'true' : 'false' }}/>
            <key>IncomingMailServerUsername</key>
            <string>{{ email|e }}</string>
            <key>OutgoingMailServerAuthentication</key>
            <string>EmailAuthPassword</string>
            <key>OutgoingMailServerHostName</key>
            <string>{{ mailServerHost|e }}</string>
            <key>OutgoingMailServerPortNumber</key>
            <integer>{{ smtpPort }}</integer>
            <key>OutgoingMailServerUseSSL</key>
            <{{ portsAreSsl ? 'true' : 'false' }}/>
            <key>OutgoingMailServerUsername</key>
            <string>{{ email|e }}</string>
            <key>OutgoingPasswordSameAsIncomingPassword</key>
            <true/>
            <key>PayloadDescription</key>
            <string>Configures email account.</string>
            <key>PayloadDisplayName</key>
            <string>{{ companyName|e }}</string>
            <key>PayloadIdentifier</key>
            <string>com.mail.{{ companyName|e }}.email1</string>
            <key>PayloadOrganization</key>
            <string>{{ companyName|e }}</string>
            <key>PayloadType</key>
            <string>com.apple.mail.managed</string>
            <key>PayloadUUID</key>
            <string>{{ uuid1 }}</string>
            <key>PayloadVersion</key>
            <integer>1</integer>
            <key>PreventAppSheet</key>
            <false/>
            <key>PreventMove</key>
            <false/>
            <key>SMIMEEnabled</key>
            <false/>
            <key>disableMailRecentsSyncing</key>
            <false/>
        </dict>
    </array>
    <key>PayloadDescription</key>
    <string>This will set up your {{ companyName|e }} mail account on your device.</string>
    <key>PayloadDisplayName</key>
    <string>{{ companyName|e }}</string>
    <key>PayloadIdentifier</key>
    <string>com.mail.{{ companyName|e }}</string>
    <key>PayloadOrganization</key>
    <string>{{ companyName|e }}</string>
    <key>PayloadRemovalDisallowed</key>
    <false/>
    <key>PayloadType</key>
    <string>Configuration</string>
    <key>PayloadUUID</key>
    <string>{{ uuid2 }}</string>
    <key>PayloadVersion</key>
    <integer>1</integer>
</dict>
</plist>

